/* Form control-group */
.control-group {
	margin-bottom: 5px;
    padding: 2px 0px;
	*zoom: 1;
}
.control-group:before, .control-group:after {
	display: table;
	line-height: 0;
	content: '';
}
.control-group:after{
	clear: both;
}

