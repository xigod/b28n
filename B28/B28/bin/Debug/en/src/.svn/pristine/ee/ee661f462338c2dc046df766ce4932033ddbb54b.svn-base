<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>behavior | urlFilter</title>
    <link rel="stylesheet" href="./public/style.css?version=4112" />
    <script src="./lang/b28n.js?version=4112"></script>
    <script src="./public/gozila.js?version=4112"></script>
    <script src="./public/menu.js?version=4112"></script>
    <script src="./public/j.js?version=4112"></script>
    <script src="./public/common.js?version=4112"></script>
</head>

<body id="urlFiterCtr">
    <div class="divbody">
        <div id="head"></div>
        <div style="width:1000px;">
            <form style="width:800px;">
                <div>
                    <table class="tabHead">
                        <tr>
                            <td>URL Filter</td>
                            <td>
                                <label class="pr20">
                                    <input type="radio" name="urlEnable" value="0">Disable</label>
                                <label class="pr20">
                                    <input type="radio" name="urlEnable" value="1">Enable</label>
                            </td>
                        </tr>
                    </table>
                </div>
                <div class="mt20" id="urlContent">
                    <div class="divLeft">
                        <div style="height:350px">
                            <ul id="groupNav">
                            </ul>
                            <ul id="addGroupUL">
                                <li>
                                    <input type="text" id="menuName" class="w120 ml10">
                                    <label class="pl20">
                                        <span class="save_icon" id="subMenu"></span>
                                        <span class="cancel_icon" id="cancelMenu"></span>
                                    </label>
                                </li>
                            </ul>
                        </div>
                        <div class="divBottom">
                            <input type="button" value="New URL Sets" id="addMenu" class="addGroup">
                        </div>
                    </div>
                    <div id="urlShow" style="float:left;margin-left:20px;width: 470px;border:solid 1px #A2A2A2">
                        <div style="height:350px">
                            <table style="width: 470px">
                                <tr id="groupListHead" class="a3">
                                    <th style="width:50px;">Item</th>
                                    <th style="width:100px;">Description</th>
                                    <th style="width:260px;">URL</th>
                                    <th style="width:60px;">Action</th>
                                </tr>
                                <tbody id="speContent" style="text-align: center;">
                                </tbody>
                            </table>
                        </div>
                        <div class="divBottom">
                            <input type="button" value="New URL" id="addUrlMenu" class="addGroup">
                        </div>
                    </div>
                </div>
            </form>
            <div class="fl w80 ml10">
                <br>
                <input type="button" class="button" value="Save" id="urlCtrSave">
                <br>
                <br>
                <input type="reset" class="button" value="Restore" onClick="window.location.reload()">
                <br>
                <br>
                <input type="button" class="button" value="Help" name="help" onclick="doHelp('qos_url')">
            </div>
        </div>
    </div>
    <script src="./js/urlFilter.js?version=4112"></script>
</body>

</html>
