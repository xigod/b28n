.slider-container .slider-right .postext {
    text-align: center;
    /* display: table-cell; */
    height:30px;
    width: 1%;
    cursor: pointer;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    float: left;
}

.slider-container .slider-right .postext .inner {
    vertical-align: middle;
}

.slider-container .slider-right .postext + .postext > .inner {
    /* border-top: #ddd 1px solid; */
}

.slider-container .slider-right .postext:hover {
    background: #fcfcfc;
}

.slider-container .slider-right .postext.active {
    background: #8484D0;
    color: #FFF;
    height:30px;
    /* background: #fcfcfc;
                border: 1px solid #F4F4F4;
                border-right: none;
                border-bottom: none;
                box-shadow: inset 1px 1px 3px rgba(0, 0, 0, 0.3); */
}

.slider-container .slider-right .postext:first-child {
    /* border-top: none !important; */
}

.slider-container img {
    width: 100%;
    height: 410px;
}

.form-container {
    max-width: 340px;
    min-width: 300px;
    margin: auto;
    border: 1px solid #e0e0e0;
    border: 1px solid rgba(224, 224, 224, 0);
    -webkit-box-shadow: 0 1px 1px 1px rgba(0, 0, 0, 0.3);
    box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, 0.3);
    -webkit-border-radius: 10px;
    border-radius: 10px;
    padding: 25px 35px;
}

.form-bottom ul {
    overflow: hidden;
    padding: 0;
}

.form-bottom ul li {
    list-style: none;
    float: left;
    padding: 0 5px;
}


.goBtn {
    padding: 16px 0;
    width: 283px;
    margin: auto;
}

.footer {
    padding: 0px;
    border-top: 1px dotted #888;
    line-height: 30px;
    position: fixed;
    bottom: 0;
    width: 100%;
    background: #444;
}

.footer .copyright {
    text-align: center;
    color: #999;
    font-size: 0.8em;
}

@media (min-width: 994px) {
    .input-box {
        margin-left: 10.333333% !important;
    }
    .other-auth {
        padding-top: 50px;
        padding-left: 30%;
    }
    #otherAuth a {
        margin: 1px 4.5%;
    }
    .login-section {
        padding-top: 50px;
    }
}

@media (max-width: 993px) {
    .login-section {
        padding-top: 0px;
    }
    .input-box {
        float: none;
    }
    .other-auth {
        margin-left: 35%;
    }
}

@media (max-width: 767px) {
    .slider {
        width: 100% ;
    }
    .slider-right {
        width: 100% !important;
    }
    
    .slider-right .postext .inner {

    }
    .slider-right .postext.active {
        border-bottom-color: #AF7DED;
    }
    .masthead .nav-bar {
        width: 100%;
        height: 40px;
        padding-right: 0;
        background: #333;
        padding-top: 7px;
    }
    #mainBody {
        padding-top: 90px;
        padding-bottom: 90px;
        position: relative;
    }
    section {
        height: auto !important;
    }
    .form-container {
        max-width: 100% !important;
    }
    .other-auth {
        padding-top: 30px;
        margin-left: 0px;
    }
    #otherAuth a {
        margin: 1px 2.5%;
    }
}

@media (max-width: 360px) {
    .authType {
        display: block;
    }
}


