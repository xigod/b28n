body {
    font-family: "微软雅黑", "Microsoft YaHei", "宋体";
}

.main-wrapper {
    position: static;
    display: table;
}

.main-wrapper .middle {
    display: table-cell;
    vertical-align: middle;
    position: static;
}

.masthead {
    height: 60px;
    background: #4e4d49;
    border-bottom: 1px solid #444;
    -webkit-box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, 0.3);
    box-shadow: 1px 1px 1px 1px rgba(0, 0, 0, 0.3);
    position: absolute;
    width: 100%;
    top: 0;
    left: 0;
    z-index: 100;
}

.masthead .logo {
    height: 85%;
    float: left;
    margin-left: 20px;
    margin-top: 5px;
}

.masthead .nav-bar {
    height: 100%;
    float: right;
    padding-right: 70px;
}

.masthead .nav-bar ul > li {
    display: inline-block;
    padding-right: 50px;
}

.masthead .nav-bar ul > li a {
    color: #ccc;
    font-weight: bold;
    font-size: 15px;
}

#mainBody {
    padding-top: 60px;
    position: fixed;
    -webkit-transition: top ease-in-out 1s;
    -moz-transition: top ease-in-out 1s;
    -o-transition: top ease-in-out 1s;
    transition: top ease-in-out 1s;
}

.slider-container {
    overflow: hidden;
    width: 100%;
    height: 364px;
    -webkit-box-shadow: 0 1px 1px 1px rgba(0, 0, 0, 0.3);
    box-shadow: 0 1px 1px 1px rgba(0, 0, 0, 0.3);
    position: relative;
}

.slider-container .slider {
    width: 100%;
    float: left;
    height: 100%;
    overflow: hidden;
    position: relative;
}

.slider-container .slider .slider-items {
    height: 100%;
    position: absolute;
    -webkit-transition: top ease-in 0.8s;
    -moz-transition: top ease-in 0.8s;
    -o-transition: top ease-in 0.8s;
    transition: top ease-in 0.8s;
    width: 100%;
}

.slider-container .slider .play-text {
    display: none;
}

.slider-container .slider-right {
    width: 100%;
    float: left;
    /* display: table; */
    background: #f3f3f3;
    position: absolute;
    bottom: 0;
}

@media (min-width: 835px) {
    .slider-container .slider-right {
        height: 30px;
    }
}

